{
  "name": "v-sevenseg",
  "version": "1.0.0",
  "description": "A vue implementation of a sevensegment display inspired by Sevenseg.js from https://github.com/BrandonLWhite/sevenSeg.js",
  "author": "Sae Woo  Nam",
  "license": "MIT",
  "repository":"https://github.com/saewoonam/vue-sevenseg.git",
  "main": "dist/v-sevenseg.js",
  "unpkg": "dist/v-sevenseg.min.js",
  "browser": {
      "./sfc": "src/components/VSevenseg.vue"
    },
  "scripts": {
    "build": "vue-cli-service build --target wc --name v-sevenseg ./src/components/VSevenseg.vue",
    "lint": "vue-cli-service lint"
  },
  "files": {
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "*.html"
  },
  "dependencies": {
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
