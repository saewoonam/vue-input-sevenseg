{
  "name": "v-input-sevenseg",
  "version": "1.0.1",
  "description": "A vue numeric input using a seven segment display inspired by Sevenseg.js from https://github.com/BrandonLWhite/sevenSeg.js",
  "author": "Sae Woo  Nam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saewoonam/vue-input-sevenseg.git"
  },
  "bugs": {
    "url": "https://github.com/saewoonam/vue-input-sevenseg/issues"
  },
  "homepage": "https://github.com/saewoonam/vue-input-sevenseg",
  "keywords": [
    "vue",
    "vue-component",
    "vue component",
    "seven",
    "segment",
    "7segment",
    "7-segment",
    "7 segment",
    "sevensegment",
    "seven-segment",
    "seven segment",
    "component",
    "numeric",
    "input"
  ],
  "main": "dist/vInputSevenseg.umd.js",
  "unpkg": "dist/vInputSevenseg.umd.min.js",
  "browser": {
    "./sfc": "src/components/VInputSevenseg.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-lib": "vue-cli-service build --target lib --name vInputSevenseg ./src/components/VInputSevenseg.vue",
    "build-wc": "vue-cli-service build --target wc --name v-input-sevenseg ./src/components/VInputSevenseg.vue",
    "build-wrapper": "vue-cli-service build --target lib --name vInputSevenseg ./src/components/wrapper.js",
    "build-index": "vue-cli-service build --target lib --name vInputSevenseg ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "*.html"
  ],
  "dependencies": {
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
